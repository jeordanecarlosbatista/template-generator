{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "module-resolver", {
                "root": ["./src"],
                "alias": {
                    "@core": "./src/core",
                    "@infrastructure": "./src/infrastructure",
                    "@interface": "./src/interface"
                }
            }
        ]
    ]
}